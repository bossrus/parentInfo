{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": true,
  "tabs": true,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketLine": false,
  "arrowFunctionParentheses": "always",
  "endOfLine": "crlf",
  "singleAttributePerLine": true
}